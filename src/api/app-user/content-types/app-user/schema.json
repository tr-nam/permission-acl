{
  "kind": "collectionType",
  "collectionName": "app_users",
  "info": {
    "singularName": "app-user",
    "pluralName": "app-users",
    "displayName": "AppUser"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password"
    },
    "app_permissions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app-permission.app-permission",
      "mappedBy": "app_users"
    },
    "app_groups": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app-group.app-group",
      "mappedBy": "app_users"
    }
  }
}
